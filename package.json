{
  "name": "@ihk-gfi/lux-components",
  "version": "11.0.0",
  "license": "IHK-GfI Open-Source-Lizenz",
  "repository": {
    "type": "git",
    "url": "https://github.com/IHK-GfI/lux-components.git"
  },
  "homepage": "https://github.com/IHK-GfI/lux-components",
  "bugs": {
    "url": "https://github.com/IHK-GfI/lux-components/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_en": "ng serve --configuration en --port 4210 --single-bundle",
    "start-ie": "ng serve --configuration es5 --port 4210 --single-bundle",
    "start-ie_en": "ng serve --configuration es5en --port 4211 --single-bundle",
    "build": "ng build --source-map",
    "build-aot": "ng build --prod --localize --single-bundle --plugin ~ie11-lazy-modules-plugin.js",
    "test": "ng test",
    "test_no_sm": "ng test --no-sourceMap",
    "test_single_run": "ng test --watch=false --browsers=ChromeHeadless",
    "test_coverage": "ng test --watch=false --code-coverage=true",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "doc": "compodoc -p src/tsconfig.app.json -n 'LUX-Components'",
    "docStart": "compodoc src -s --port 8888",
    "security": "npm audit --registry=https://registry.npmjs.org --audit-level high",
    "smoketest": "npm run test_single_run && npm run build-aot && npm run lint --bailOnLintError true",
    "xi18n": "ng xi18n --output-path src/locale --ivy"
  },
  "private": false,
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.10",
    "@angular-eslint/builder": "4.0.0",
    "@angular-eslint/eslint-plugin": "4.0.0",
    "@angular-eslint/eslint-plugin-template": "4.0.0",
    "@angular-eslint/schematics": "4.0.0",
    "@angular-eslint/template-parser": "4.0.0",
    "@angular/animations": "11.2.11",
    "@angular/cdk": "11.2.10",
    "@angular/cli": "11.2.10",
    "@angular/common": "11.2.11",
