    "@angular/material": "8.2.0",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@compodoc/compodoc": "1.1.10",
    "@fortawesome/fontawesome-free": "5.11.2",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.5",
    "codelyzer": "5.1.1",
    "core-js": "3.2.1",
    "cpy-cli": "2.0.0",
